1.  SELECT * FROM actor;
2.  SELECT last_name FROM actor WHERE first_name = 'john';
3.  SELECT * FROM actor WHERE last_name = 'neeson';
4.  SELECT * FROM actor WHERE (actor_id % 10)=0;
5.  SELECT description FROM film_text WHERE film_id = 100;
6.  SELECT * FROM film WHERE rating = 'r';
7.  SELECT * FROM film WHERE rating != 'r';
8.  SELECT * FROM film ORDER BY length DESC LIMIT 10;
9.  SELECT * FROM film WHERE length=(SELECT MAX(length) FROM film);
10. SELECT * FROM film WHERE special_features like '%Deleted Scenes%';
11. SELECT * FROM actor GROUP BY last_name HAVING COUNT(*) <= 1 ORDER BY last_name DESC;
12. SELECT last_name, COUNT(*) AS magnitude FROM actor GROUP BY last_name HAVING COUNT(*) > 1 ORDER BY magnitude DESC;
13. SELECT actor_id, COUNT(*) AS magnitude FROM film_actor GROUP BY actor_id ORDER BY magnitude DESC LIMIT 1;
14. SELECT rental.return_date FROM rental JOIN inventory ON rental.inventory_id=inventory.inventory_id WHERE film_id=(SELECT film_id FROM film WHERE title='Academy Dinosaur');
15. SELECT AVG(length) FROM film;
16. SELECT category, AVG(length) FROM film_list GROUP BY category;
17. SELECT title FROM film_list WHERE description LIKE '%robot%';
18. SELECT title FROM film WHERE release_year=2010;
19. SELECT title FROM film_list WHERE category='horror';
20. SELECT first_name, last_name FROM staff WHERE staff_id=2;
21. SELECT film_info FROM actor_info WHERE first_name='fred' AND last_name='costner';
22. SELECT COUNT(*) FROM country;
23. SELECT name FROM language ORDER BY name DESC;
24. SELECT first_name, last_name FROM actor WHERE last_name like '%son' ORDER BY first_name ASC;
25. SELECT category, COUNT(*) AS magnitude FROM film_list GROUP BY category ORDER BY magnitude DESC LIMIT 1;